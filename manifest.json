{
    "manifest_version": 2,
  
    "name": "Real THONKEEERS",
    "description": "GET THONKERS EVERYWHERE",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "index.html"
    },
    "options_page": "options.html",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "content_scripts": [
        {
            "matches":["http://*/*", "https://*/*"],
            "css": ["thonk.css"],
            "js": ["thonk.js", "thonkmanager.js"]
        }
    ]
  }